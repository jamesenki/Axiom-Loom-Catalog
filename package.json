  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "build:analyze": "ANALYZE=true react-app-rewired build",
    "test": "react-app-rewired test",
    "test:coverage": "react-app-rewired test --coverage --watchAll=false",
    "test:watch": "react-app-rewired test --watchAll",
    "test:ci": "react-app-rewired test --ci --coverage --watchAll=false",
    "test:debug": "react-app-rewired --inspect-brk test --runInBand --no-cache",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:deployment": "playwright test e2e/post-deployment-smoke-test.spec.ts",
    "eject": "react-scripts eject",
    "server": "node src/server.js",
    "dev": "concurrently \"npm run server\" \"npm run start\"",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint && npm run type-check && npm run test:ci",
    "serve:ci": "npx serve -s build -l 3000",
    "precommit": "npm run lint && npm run type-check",
    "deploy:staging": "echo 'Deploying to staging...'",
    "deploy:production": "echo 'Deploying to production...'",
    "security:audit": "npm audit --audit-level=moderate",
    "production": "concurrently \"npm run server\" \"npx serve -s build -l ${PORT:-3000}\"",
    "production:pm2": "pm2 start ecosystem.config.js",
    "production:stop": "pm2 stop all",
    "deploy": "./deploy.sh",
    "performance:test": "npx lhci autorun",
    "validate:build": "./scripts/validate-build.sh",
    "test:regression": "playwright test playwright/e2e/regression-suite.spec.ts",
    "test:e2e:headed": "playwright test --headed",
    "test:full": "npm run test:ci && npm run test:e2e",
    "test:ip": "npx playwright test ai-predictive-maintenance-workflow.spec.ts --config=playwright.config.ip.ts",
    "test:ip:enhanced": "npx playwright test ai-predictive-maintenance-enhanced.spec.ts --config=playwright.config.ip.ts",
    "test:ip:headed": "npx playwright test ai-predictive-maintenance-workflow.spec.ts --config=playwright.config.ip.ts --headed",
    "test:ip:debug": "npx playwright test ai-predictive-maintenance-enhanced.spec.ts --config=playwright.config.ip.ts --headed --debug"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
